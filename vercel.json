{
  "version": 2,
  "builds": [
    {
      "src": "*.html",
      "use": "@vercel/static"
    },
    {
      "src": "*.js",
      "use": "@vercel/static"
    },
    {
      "src": "public/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/news.html",
      "dest": "/news.html"
    },
    {
      "src": "/news",
      "dest": "/news.html"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}